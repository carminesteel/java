package ex01;

public class Sample {

	public static void main(String[] args) {
		//배열에 있는 데이터들에 대한 평균값과 최대값
		int sum = 0;
		int[] num = {96,64,36,46,14,31,45,14};
		for(int i=0; i<=num.length-1; i++) {
			sum = sum + num[i];
		}
		double avg=(double) sum/num.length;
		System.out.println("평균 : " + avg);
	}

}
