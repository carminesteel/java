package ex02;

public class Sample {

	public static void main(String[] args) {
		Calculator c1 = new Calculator();
		c1.r=5;
		double area=c1.areaCircle();
		System.out.println("원의 넓이 : "+area);

		
		Computer com = new Computer();
		com.r=5;
		System.out.println("더 정확한 원의 넓이 :"+com.areaCircle());
	}

}
