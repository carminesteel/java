package ex01;

public class Sample {

	public static void main(String[] args) {
			System.out.println("안녕");
			System.out.println("안녕녕");
			
			//정수형변수
			int num1=110;
			int num2=200;
			int num3=num1+num2;
			int num4 = num3 * num1;
			int num5 = num2 / num1;
			System.out.println("num1=" + num1);
			System.out.println("num2=" + num2);
			System.out.println("두수를더한결과 = "+num3);
			System.out.println("넘삼곱하기넘일 = "+ num4);
			System.out.println("넘이나누기넘일 = "+num5);
	}

}
