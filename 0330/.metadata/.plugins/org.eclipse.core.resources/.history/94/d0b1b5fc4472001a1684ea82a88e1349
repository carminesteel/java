package main;

import java.util.Scanner;

public class Main {

	public static void main(String[] args){
		Scanner s = new Scanner(System.in);			
		int A = 0;
		A = s.nextInt();
		int B = 0;
		B = s.nextInt();
		if(A>B) {
			System.out.println(">");
		}else if(A<B){
			System.out.println("<");
		}else if(A==B) {
			System.out.println("==");
		}
	}
}
