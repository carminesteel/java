package main;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		int yun = 0;
		int year = 0;
		Scanner s = new Scanner(System.in);		
		System.out.println("¼ıÀÚ ÃÄ");		
		year = s.nextInt();
		if(year%4==0){
			if(year%100!=0&&year%400==0) {
				yun = 1;
				System.out.println(yun);	
			}
		}else {
		yun = 0;
		System.out.println(yun);
		}			
		
	}

}
